    },
    "[json]": {

    },
    "[yaml]": {

    },
    "doxdocgen.generic.authorTag": "@author {Paul McCarthy} ({paulmccarthy676@gmail.com})",
    "doxdocgen.generic.briefTemplate": "@brief {Put a brief explanation of code here}",
    "doxdocgen.generic.authorName": "Paul McCarthy",
    "doxdocgen.generic.authorEmail": "paulmccarthy676@gmail.com",
    "todo.indentation": "",
    "todo.timekeeping.created.enabled": true,
    "todo.tags.names": [
        "critical",
        "high",
        "low",
        "today",
        "idea",
        "note",
        "reminder"
    ],
    "todo.file.defaultContent": "Todo:  ‚òê Item @created"
}
